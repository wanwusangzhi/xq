<!--src/components/column/index.wxml-->
<view class="box">
  <view wx:for="{{ listData }}" wx:key="{{ index }}" id="{{index}}" class="box_items">
    <image mode="scaleToFill" src="{{ item.src || '/assets/default_column.png' }}"></image>
    <view class="text">{{ item.name }}</view>
  </view>
</view>